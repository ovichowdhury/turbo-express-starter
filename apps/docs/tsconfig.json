{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@components/*": ["components/*"],
      "@pages/*": ["pages/*"],
      "@redux/*": ["redux/*"],
      "@utils/*": ["utils/*"],
      "@styles/*": ["styles/*"],
      "@urls/*": ["urls/*"]
    }
  },
  "extends": "tsconfig/nextjs.json",
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
